{
  "dart.flutterSdkPath": "",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "files.exclude": {
    "**/.dart_tool": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true,
    "**/build": true
  },
  "search.exclude": {
    "**/.dart_tool": true,
    "**/build": true,
    "**/ios/Pods": true
  }
}